@page "{pageNumber:int?}"
@using GDLevels.Pages.Shared.Components
@model AdminModel
@{
    ViewData["Title"] = "Админ-панель";
}

@if (Model.IsAuthorized)
{
    <component type="typeof(LevelsTable)" render-mode="Static" param-LevelsAdapter="@Model.LevelsAdapter" param-CurrentPage="@Model.PageNumber" param-LevelsPerPage="30" param-DisplayHeader="true" param-PageName="@($"admin/{Model.PageNumber}")" param-CanDeleteLevels="true"/>
}
